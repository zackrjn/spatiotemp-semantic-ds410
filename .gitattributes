# Normalize text files
*.py text eol=lf
*.sh text eol=lf
*.md text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.ipynb text eol=lf

# Large assets via Git LFS
data/samples/** filter=lfs diff=lfs merge=lfs -text
*.warc.gz filter=lfs diff=lfs merge=lfs -text
*.wet.gz  filter=lfs diff=lfs merge=lfs -text
*.wat.gz  filter=lfs diff=lfs merge=lfs -text
*.bin     filter=lfs diff=lfs merge=lfs -text
*.model   filter=lfs diff=lfs merge=lfs -text


